@import "/templates/cart/item/cart-item-template.wxss";

/*基础容器*/
.container{
    background-color: #f2f2f2;
}


/*
* 底部购买栏样式
*/
.cart-bar-container{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    position: fixed;
    bottom: 0;
    height: 100rpx;
    width: 100%;
    background: #FEFEFE;
    border-top: 1px solid #DCDCDC;
    border-bottom: 1px solid #EEEEEE;
    z-index: 100;
}
