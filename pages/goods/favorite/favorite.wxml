<!--收藏列表页面-->
<!--依赖：收藏项目列表-->
<import src="/templates/goods/fav/goods-fav-template.wxml" />

<view class="container">
    <block wx:for="{{favorites}}" wx:key="goods_id">
        <template is="goodsFavTemplate" data="{{...item}}" />
    </block>
</view>

<!--占位提示-->
<view class="order-empty-tips" wx:if="{{favorites.length == 0}}">
    <image src="/images/icons/order-empty.png" />
    <text>您还没有收藏商品</text>
</view>