page{
    height: 100%;
    background-color: #f2f2f2;
}

.header {
  background-color: #11ca6f;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.header .title {
  font-size: 34rpx;
  font-weight: bold;
  margin-top: 50px;
}

.header .describe {
  margin-top: 20px;
}

.header .contact {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 160rpx;
  height: 60rpx;
  background-color: #FFF;
  color: #11ca6f;
  margin-top: 20px;
  font-size: 28rpx;
}

.header .preview {
  width: 700rpx;
  margin-top: 20px;
}

.header text {
  color: #FFF;
}

.content .solution {
  width: 100%;
}
